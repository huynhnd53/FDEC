                        <button type="button" class="btn btn-light" id="newOrder"><a href="/Order/CreateOrder" style="text-decoration: none;"><i class="fas fa-box icon"></i>Gửi hàng ngay</a></button>
                    </div>
                </div>
                <div id="task_bottom">
                    <div class="task_b">
                        <div class="input-group flex-nowrap button_task">
                            <span class="input-group-text" id="addon-wrapping">Số Hàng</span>
                            <input type="text" class="form-control" placeholder="Số Hàng" aria-label="Username" aria-describedby="addon-wrapping">
